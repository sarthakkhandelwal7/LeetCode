    "class Solution:\n",
    "    def flipAndInvertImage(self, a):\n",
    "        for i in a:\n",
    "            i.reverse()\n",
    "        for i in a:\n",
    "            for j in range(len(i)):\n",
    "                if i[j] == 0:\n",
    "                    i[j] = 1\n",
    "                else:\n",
    "                    i[j] = 0"
